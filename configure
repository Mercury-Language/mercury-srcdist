    *cygwin*)
        MATH_LIB=
        if test "$enable_symlinks_opt" != "yes"; then
            MMC_USE_SYMLINKS_OPT=--no-use-symlinks
            LN_S=false
        fi
        ;;
    *mingw*)
        MATH_LIB=
        if test "$enable_symlinks_opt" != "yes"; then
            MMC_USE_SYMLINKS_OPT=--no-use-symlinks
            LN_S=false
        fi
        ;;
esac





