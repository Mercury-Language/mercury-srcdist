  $as_echo_n "(cached) " >&6
else
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

            #include <sys/types.h>
            #include <sys/times.h>

int
main ()
{

            struct tms t;
            long Ut, St, CUt, CSt;
            long Ret;

            Ret = (long) times(&t);

            Ut = (long) t.tms_utime;
            St = (long) t.tms_stime;
            CUt = (long) t.tms_cutime;
            CSt = (long) t.tms_cstime;

  ;
  return 0;
}
_ACEOF
