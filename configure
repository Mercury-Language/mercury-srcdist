            #endif

            #define FAULT_ADDRESS ((int *)112)

            int main() {
                signal(SIGSEGV, (void (*)(int))handler);

                /* provoke a SIGSEGV */
                (*FAULT_ADDRESS)++;

                exit(1);
            }

            #if MR_HAVE_SIGCONTEXT_STRUCT_3ARG
            void handler(int signum, int code, struct sigcontext_struct context) {
            #else
