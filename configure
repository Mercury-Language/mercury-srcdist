            then
                cshpath="$dir/$prog"
            fi
        fi
    done
done

if test "$cshpath" = ""
then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    CSHPATH="no csh path"
else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $cshpath" >&5
$as_echo "$cshpath" >&6; }
    CSHPATH="$cshpath"
fi


