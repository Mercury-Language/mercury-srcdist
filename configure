$as_echo_n "checking whether we can use thread-local storage class extension... " >&6; }
if ${mercury_cv_thread_local_storage+:} false; then :
  $as_echo_n "(cached) " >&6
else
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
