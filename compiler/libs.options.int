:- type option_table == getopt_io.option_table(libs.options.option).
:- pred inconsequential_options(set.set(libs.options.option)).
:- mode inconsequential_options((builtin.out)) is det.
:- pred long_option(string, libs.options.option).
:- mode long_option((builtin.in), (builtin.out)) is semidet.
:- pred option_defaults(libs.options.option, getopt_io.option_data).
:- mode option_defaults((builtin.out), (builtin.out)) is nondet.
:- pred option_table_add_mercury_library_directory(string, libs.options.option_table, libs.options.option_table).
:- mode option_table_add_mercury_library_directory((builtin.in), (builtin.in), (builtin.out)) is det.
