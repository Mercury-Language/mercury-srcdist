:- type_representation(module_name_ext, is_word_aligned_ptr).
:- type_representation(rebuild_module_deps, du_repn(enum("do_rebuild_module_deps", "do_not_rebuild_module_deps", [],  no_c_j_cs))).
:- type_representation(target_file, is_word_aligned_ptr).
