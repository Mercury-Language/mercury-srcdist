:- type_representation(mode_error_info, is_word_aligned_ptr).
:- type_representation(mode_warning_info, is_word_aligned_ptr).
:- type_representation(negated_context_desc, du_repn(enum("if_then_else", "negation", [],  no_c_j_cs))).
:- type_representation(pred_id_var_multimode_error, is_word_aligned_ptr).
