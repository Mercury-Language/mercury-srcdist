:- module (check_hlds.mode_errors).
:- interface.
:- type cannot_create_reason
	where	type_is_abstract_enum(1).
:- type delayed_goal.
:- type final_inst_error
	where	type_is_abstract_enum(2).
:- type merge_context
	where	type_is_abstract_enum(2).
:- type merge_error.
:- type mode_error.
:- type mode_error_info.
:- type mode_error_unify_rhs.
:- type mode_warning.
:- type mode_warning_info.
:- type negated_context_desc
	where	type_is_abstract_enum(1).
:- type pred_id_var_multimode_error.
:- type schedule_culprit.
:- type var_multimode_error.
:- type_representation(cannot_create_reason, du_repn(enum("cannot_init_any", "cannot_deep_copy_partial_term", [],  no_c_j_cs))).
:- type_representation(delayed_goal, is_word_aligned_ptr).
:- type_representation(final_inst_error, du_repn(enum("too_instantiated", "not_instantiated_enough", ["wrongly_instantiated"],  no_c_j_cs))).
:- type_representation(merge_context, du_repn(enum("merge_disj", "merge_if_then_else", ["merge_stm_atomic"],  no_c_j_cs))).
