:- module make.
:- interface.
:- include_module (make.build).
:- include_module (make.options_file).
:- type make_info.
:- type maybe_invoked_by_mmc_make
	where	type_is_abstract_enum(1).
:- type rebuild_module_deps
	where	type_is_abstract_enum(1).
:- type_representation(file_timestamps, is_eqv_to(map(string, maybe_error(timestamp)))).
:- type_representation(make_info, is_word_aligned_ptr).
:- type_representation(maybe_invoked_by_mmc_make, du_repn(enum("not_invoked_by_mmc_make", "invoked_by_mmc_make", [],  no_c_j_cs))).
:- type_representation(rebuild_module_deps, du_repn(enum("do_rebuild_module_deps", "do_not_rebuild_module_deps", [],  no_c_j_cs))).
