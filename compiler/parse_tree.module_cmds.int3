:- module (parse_tree.module_cmds).
:- interface.
:- type command_verbosity
	where	type_is_abstract_enum(1).
:- type quote_char
	where	type_is_abstract_enum(1).
:- type update_interface_result
	where	type_is_abstract_enum(2).
:- type_representation((parse_tree.module_cmds).command_verbosity, du_repn(enum("cmd_verbose", "cmd_verbose_commands", [], []))).
:- type_representation((parse_tree.module_cmds).quote_char, du_repn(enum("forward", "double", [], []))).
:- type_representation((parse_tree.module_cmds).update_interface_result, du_repn(enum("interface_new_or_changed", "interface_unchanged", ["interface_error"], []))).
