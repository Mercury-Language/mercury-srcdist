:- module (libs.trace_params).
:- interface.
:- type maybe_exec_trace_enabled
	where	type_is_abstract_enum(1).
:- type trace_level.
:- type trace_suppress_items.
:- type_representation(maybe_exec_trace_enabled, du_repn(enum("exec_trace_is_not_enabled", "exec_trace_is_enabled", [],  no_c_j_cs_e))).
:- type_representation(trace_level, du_repn(enum("none", "basic", ["basic_user", "shallow", "deep", "decl_rep"],  no_c_j_cs_e))).
:- type_representation(trace_suppress_items, is_eqv_to(set(trace_suppress_item))).
