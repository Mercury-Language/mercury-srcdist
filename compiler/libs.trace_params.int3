:- module (libs.trace_params).
:- interface.
:- type eff_trace_level.
:- type maybe_exec_trace_enabled
	where	type_is_abstract_enum(1).
:- type trace_level.
:- type trace_suppress_items.
:- type_representation(eff_trace_level, du_repn(enum("eff_none", "eff_basic", ["eff_basic_user", "eff_shallow", "eff_deep", "eff_decl_rep"],  no_c_j_cs))).
:- type_representation(maybe_exec_trace_enabled, du_repn(enum("exec_trace_is_not_enabled", "exec_trace_is_enabled", [],  no_c_j_cs))).
:- type_representation(trace_level, du_repn(enum("none", "shallow", ["deep", "decl_rep"],  no_c_j_cs))).
:- type_representation(trace_suppress_items, is_eqv_to(set(trace_suppress_item))).
