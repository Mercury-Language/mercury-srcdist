:- module (parse_tree.file_kind).
:- interface.
:- type file_kind.
:- type int_file_kind
	where	type_is_abstract_enum(2).
:- type int_or_opt_file_kind.
:- type opt_file_kind
	where	type_is_abstract_enum(1).
:- type src_file_kind.
:- type_representation(int_file_kind, du_repn(enum("ifk_int0", "ifk_int1", ["ifk_int2", "ifk_int3"],  no_c_j_cs))).
:- type_representation(opt_file_kind, du_repn(enum("ofk_opt", "ofk_trans_opt", [],  no_c_j_cs))).
:- type_representation(src_file_kind, du_repn(direct_dummy("sfk_src",  no_c_j_cs))).
