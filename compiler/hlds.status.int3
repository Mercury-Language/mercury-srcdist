:- module hlds.status.
:- interface.
:- type inst_status.
:- type instance_status.
:- type instmode_export
	where	type_is_abstract_enum(2).
:- type instmode_import
	where	type_is_abstract_enum(2).
:- type item_export
	where	type_is_abstract_enum(2).
:- type item_import.
:- type item_mercury_status.
:- type maybe_opt_imported
	where	type_is_abstract_enum(1).
:- type mode_status.
:- type new_instmode_status.
:- type old_import_status.
:- type pred_status.
:- type type_status.
:- type typeclass_status.
:- type_representation(inst_status, du_repn(notag("inst_status", new_instmode_status,  no_c_j_cs))).
:- type_representation(instance_status, du_repn(notag("instance_status", old_import_status,  no_c_j_cs))).
:- type_representation(instmode_export, du_repn(enum("instmode_export_nowhere", "instmode_export_only_submodules", ["instmode_export_anywhere"],  no_c_j_cs))).
:- type_representation(instmode_import, du_repn(enum("instmode_import_plain", "instmode_import_abstract", ["instmode_import_opt"],  no_c_j_cs))).
:- type_representation(item_export, du_repn(enum("item_export_nowhere", "item_export_only_submodules", ["item_export_anywhere"],  no_c_j_cs))).
:- type_representation(maybe_opt_imported, du_repn(enum("is_not_opt_imported", "is_opt_imported", [],  no_c_j_cs))).
:- type_representation(mode_status, du_repn(notag("mode_status", new_instmode_status,  no_c_j_cs))).
:- type_representation(pred_status, du_repn(notag("pred_status", old_import_status,  no_c_j_cs))).
:- type_representation(type_status, du_repn(notag("type_status", old_import_status,  no_c_j_cs))).
:- type_representation(typeclass_status, du_repn(notag("typeclass_status", old_import_status,  no_c_j_cs))).
