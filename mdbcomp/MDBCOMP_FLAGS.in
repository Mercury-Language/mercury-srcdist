--c-include-directory ../library
--c-include-directory ../library/Mercury/mihs
--csharp-flag -keyfile:../mercury.snk
--erlang-include-directory ../library/Mercury/hrls
-L../boehm_gc
-L../runtime
-L../library
-L../trace
-L../mdbcomp
-L../browser
--no-java-classpath
