:- module stm_builtin.
:- interface.
:- type rollback_exception
	where	type_is_abstract_enum(1).
:- type stm.
:- type stm_dummy_output.
:- type stm_validation_result
	where	type_is_abstract_enum(1).
:- type stm_var(T).
:- type_representation(stm_builtin.rollback_exception, du_repn(enum("rollback_invalid_transaction", "rollback_retry", [], []))).
:- type_representation(stm_builtin.stm, du_repn(notag("stm", [c("MR_STM_TransLog *", [can_pass_as_mercury_type])]))).
:- type_representation(stm_builtin.stm_dummy_output, du_repn(direct_dummy("stm_dummy_output", []))).
:- type_representation(stm_builtin.stm_validation_result, du_repn(enum("stm_transaction_valid", "stm_transaction_invalid", [], []))).
:- type_representation(stm_builtin.stm_var(V_1), du_repn(notag("tvar", [c("MR_STM_Var *", [can_pass_as_mercury_type, stable])]))).
