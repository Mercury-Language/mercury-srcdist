:- mode write_variable_anon_vars((builtin.in), (builtin.in), (builtin.in), (builtin.in), (builtin.out), (builtin.in), (builtin.out), (builtin.di), (builtin.uo)) is det.

term_io.escaped_char(V_3) = V_4 :-
    term_io.string_is_escaped_char(V_3, V_4).

term_io.escaped_string(V_3) = V_2 :-
    V_2 = string.append_list(V_4),
    V_4 = list.reverse(V_5),
    V_5 = string.foldl(V_6, V_3, V_7),
    V_6 = term_io.add_escaped_char : (func(character, list.list(string)) = list.list(string)),
    V_7 = list.[] : list.list(string).

term_io.format_constant(V_3) = V_2 :-
    V_2 = term_io.format_constant_agt(V_3, V_4),
    V_4 = term_io.not_adjacent_to_graphic_token : term_io.adjacent_to_graphic_token.

term_io.quote_atom(V_4, V_6, V_7) :-
    V_8 = term_io.not_adjacent_to_graphic_token : term_io.adjacent_to_graphic_token,
    term_io.quote_atom_agt(V_4, V_8, V_6, V_7).

term_io.quote_atom(V_5, V_6, V_8, V_9) :-
    V_10 = term_io.not_adjacent_to_graphic_token : term_io.adjacent_to_graphic_token,
    term_io.quote_atom_agt(V_5, V_6, V_10, V_8, V_9).

term_io.quote_atom_agt(V_5, V_6, V_9, V_10) :-
    io.output_stream(V_8, V_9, V_11),
    term_io.quote_atom_agt(V_8, V_5, V_6, V_11, V_10).

term_io.quote_char(V_4, V_7, V_8) :-
    io.output_stream(V_6, V_7, V_9),
    V_10 = term_io.quoted_char(V_4),
    io.write_string(V_6, V_10, V_9, V_8).

term_io.quote_char(V_5, V_6, V_8, V_9) :-
    V_10 = term_io.quoted_char(V_6),
    stream.put(V_5, V_10, V_8, V_9).

term_io.quote_string(V_4, V_7, V_8) :-
    io.output_stream(V_6, V_7, V_9),
    term_io.quote_string(V_6, V_4, V_9, V_8).

term_io.quoted_atom(V_3) = V_2 :-
    V_2 = term_io.quoted_atom_agt(V_3, V_4),
    V_4 = term_io.not_adjacent_to_graphic_token : term_io.adjacent_to_graphic_token.

term_io.write_constant(V_4, V_7, V_8) :-
    io.output_stream(V_6, V_7, V_9),
    term_io.write_constant(V_6, V_4, V_9, V_8).

term_io.write_constant(V_5, V_6, V_8, V_9) :-
    V_10 = term_io.not_adjacent_to_graphic_token : term_io.adjacent_to_graphic_token,
    term_io.write_constant(V_5, V_6, V_10, V_8, V_9).

term_io.write_escaped_char(V_4, V_7, V_8) :-
    io.output_stream(V_6, V_7, V_9),
    term_io.write_escaped_char(V_6, V_4, V_9, V_8).

term_io.write_escaped_string(V_4, V_7, V_8) :-
    io.output_stream(V_6, V_7, V_9),
    term_io.write_escaped_string(V_6, V_4, V_9, V_8).

term_io.write_escaped_string(V_5, V_6, V_8, V_9) :-
    V_10 = term_io.write_escaped_char(V_5) : pred(character, State_2, State_2),
    string.foldl(V_10, V_6, V_8, V_9).

term_io.write_term(V_5, V_6, V_9, V_10) :-
    io.output_stream(V_8, V_9, V_11),
    term_io.write_term(V_8, V_5, V_6, V_11, V_10).

term_io.write_term(V_6, V_7, V_8, V_11, V_12) :-
    V_10 = ops.init_mercury_op_table,
    term_io.write_term_with_op_table(V_6, V_10, V_7, V_8, V_11, V_12).

term_io.write_term_anon_vars(V_10, V_11, V_12, V_16, V_17, V_18, V_19, V_20, V_21) :-
    V_22 = ops.universal_priority(V_11),
    term_io.write_term_prio_anon_vars(V_10, V_11, V_12, V_22, V_16, V_17, V_18, V_19, V_20, V_21).

term_io.write_term_nl(V_5, V_6, V_9, V_10) :-
    io.output_stream(V_8, V_9, V_11),
    term_io.write_term_nl(V_8, V_5, V_6, V_11, V_10).

term_io.write_term_nl(V_6, V_7, V_8, V_11, V_12) :-
    V_10 = ops.init_mercury_op_table,
    term_io.write_term_nl_with_op_table(V_6, V_10, V_7, V_8, V_11, V_12).

term_io.write_term_nl_with_op_table(V_6, V_7, V_8, V_11, V_12) :-
    io.output_stream(V_10, V_11, V_13),
    term_io.write_term_nl_with_op_table(V_10, V_6, V_7, V_8, V_13, V_12).

term_io.write_term_nl_with_op_table(V_7, V_8, V_9, V_10, V_12, V_13) :-
    term_io.write_term_with_op_table(V_7, V_8, V_9, V_10, V_12, V_14),
    V_15 = ".\n" : string,
    io.write_string(V_7, V_15, V_14, V_13).

term_io.write_term_with_op_table(V_6, V_7, V_8, V_11, V_12) :-
    io.output_stream(V_10, V_11, V_13),
    term_io.write_term_with_op_table(V_10, V_6, V_7, V_8, V_13, V_12).

term_io.write_term_with_op_table(V_7, V_8, V_9, V_10, V_14, V_15) :-
    V_16 = 0 : int,
    term_io.write_term_anon_vars(V_7, V_8, V_10, V_9, V_12, V_16, V_13, V_14, V_15).

term_io.write_variable(V_5, V_6, V_9, V_10) :-
    io.output_stream(V_8, V_9, V_11),
    term_io.write_variable(V_8, V_5, V_6, V_11, V_10).

term_io.write_variable(V_6, V_7, V_8, V_11, V_12) :-
    V_10 = ops.init_mercury_op_table,
