:- func term_io.add_escaped_char(character, list.list(string)) = list.list(string).
:- mode add_escaped_char((builtin.in), (builtin.in)) = (builtin.out) is det.
:- func term_io.format_constant_agt(term.const, term_io.adjacent_to_graphic_token) = string.
:- mode format_constant_agt((builtin.in), (builtin.in)) = (builtin.out) is det.
:- pred term_io.write_constant(io.output_stream, term.const, term_io.adjacent_to_graphic_token, io.state, io.state).
