:- pred term_io.write_term_anon_vars(io.output_stream, OpTable, term.term(T), varset.varset(T), varset.varset(T), int, int, io.state, io.state) <= (ops.op_table(OpTable)).
:- mode write_term_anon_vars((builtin.in), (builtin.in), (builtin.in), (builtin.in), (builtin.out), (builtin.in), (builtin.out), (builtin.di), (builtin.uo)) is det.
:- pred term_io.write_term_prio_anon_vars(io.output_stream, OpTable, term.term(T), ops.priority, varset.varset(T), varset.varset(T), int, int, io.state, io.state) <= (ops.op_table(OpTable)).
