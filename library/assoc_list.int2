:- module assoc_list.
:- interface.
:- use_module list.
:- use_module pair.
:- type assoc_list(T) == list.list(pair.pair(T, T)).
:- type assoc_list(K, V) == list.list(pair.pair(K, V)).
:- type_representation(assoc_list(V_1), is_eqv_to(list.list(pair.pair(V_1, V_1)))).
:- type_representation(assoc_list(V_1, V_2), is_eqv_to(list.list(pair.pair(V_1, V_2)))).
