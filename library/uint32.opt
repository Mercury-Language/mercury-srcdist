:- pragma inline((uint32.(/))/2).
uint32.(V_4 / V_5) = V_3 :-
    V_3 = uint32.(V_4 // V_5).

:- pragma inline((uint32.(//))/2).
uint32.(V_4 // V_5) = V_6 :-
    ( if
      V_5 = 0u32 : uint32
    then
      V_7 = math.domain_error(V_8) : math.domain_error,
      V_8 = "uint32.\'//\': division by zero" : string,
      exception.throw(V_7)
    else
      V_6 = uint32.unchecked_quotient(V_4, V_5)
    ).

