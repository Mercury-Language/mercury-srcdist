:- pragma inline(func((uint16.(/))/2)).
uint16.(V_4 / V_5) = V_3 :-
    V_3 = uint16.(V_4 // V_5).

:- pragma inline(func((uint16.(//))/2)).
uint16.(V_4 // V_5) = V_6 :-
    ( if
      V_5 = 0u16 : uint16
    then
      V_7 = exception.domain_error(V_8) : exception.domain_error,
      V_8 = "uint16.\'//\': division by zero" : string,
      exception.throw(V_7)
    else
      V_6 = uint16.unchecked_quotient(V_4, V_5)
    ).

