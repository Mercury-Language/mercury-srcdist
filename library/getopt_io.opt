getopt_io.expand_file_specials(V_9, V_10, V_11, V_12, V_13, V_14, V_16, V_17) :-
    V_18 = set.init,
    getopt_io.expand_file_specials_check(V_9, V_10, V_11, V_12, V_18, V_13, V_14, V_16, V_17).

getopt_io.expand_file_specials_check(V_10, V_11, V_12, V_13, V_14, V_15, V_16, V_19, V_20) :-
    V_21 = getopt_io.go_empty_cord : getopt_io.go_cord(getopt_io.option_value(OptionType_1)),
    getopt_io.expand_file_specials_loop(V_10, V_11, V_12, V_13, V_14, V_15, V_21, V_18, V_19, V_20),
    V_16 = getopt_io.go_cord_to_list(V_18).

getopt_io.init_option_table(V_3, V_4) :-
    V_9 = (pred(V_10::(free >> ground)) is nondet :-
      some [] ( % compiler
        call(V_3, V_6, V_7),
        V_11 = pair.(V_6 - V_7) : pair.pair(OptionType_1, getopt_io.option_data),
        V_10 = V_11
      )
