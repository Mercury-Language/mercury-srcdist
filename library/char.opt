char.to_int(V_3) = V_4 :-
    char.to_int(V_3, V_4).

char.to_lower(V_3, V_4) :-
    ( if
      char.lower_upper(V_5, V_3)
    then
      V_4 = V_5
    else
      V_4 = V_3
    ).

char.to_lower(V_3) = V_4 :-
    char.to_lower(V_3, V_4).

char.to_upper(V_3, V_4) :-
    ( if
      char.lower_upper(V_3, V_5)
    then
      V_4 = V_5
    else
      V_4 = V_3
    ).

char.to_upper(V_3) = V_4 :-
