:- pragma inline((int16.(/))/2).
int16.(V_4 / V_5) = V_3 :-
    V_3 = int16.(V_4 // V_5).

:- pragma inline((int16.(//))/2).
int16.(V_4 // V_5) = V_6 :-
    ( if
      V_5 = 0i16 : int16
    then
      V_7 = math.domain_error(V_8) : math.domain_error,
      V_8 = "int16.\'//\': division by zero" : string,
      exception.throw(V_7)
    else
      V_6 = int16.unchecked_quotient(V_4, V_5)
    ).

