:- pragma inline((int16.even)/1).
int16.even(V_2) :-
    V_3 = int16.(V_2 /\ V_4),
    V_4 = 1i16 : int16,
    V_3 = 0i16 : int16.

int16.from_bytes_be(V_4, V_5) = V_3 :-
    V_3 = int16.from_bytes_le(V_5, V_4).

:- pragma foreign_proc("C", int16.from_bytes_le(LSB :: (builtin.in), MSB :: (builtin.in)) = (I16 :: (builtin.out)), [will_not_call_mercury, thread_safe, not_tabled_for_io, promise_pure, will_not_modify_trail], "
    unsigned char *int16_bytes = (unsigned char *) &I16;
#if defined(MR_BIG_ENDIAN)
    int16_bytes[0] = MSB;
    int16_bytes[1] = LSB;
#else
    int16_bytes[0] = LSB;
    int16_bytes[1] = MSB;
#endif
").

int16.int16_to_doc(V_3) = pretty_printer.str(V_4) :-
    V_4 = string.int16_to_string(V_3).

int16.max(V_4, V_5) = V_3 :-
    ( if
      int16.(V_4 > V_5)
    then
      V_3 = V_4
    else
      V_3 = V_5
    ).

int16.max_int16 = 32767i16.

int16.min(V_4, V_5) = V_3 :-
    ( if
      int16.(V_4 < V_5)
    then
      V_3 = V_4
    else
      V_3 = V_5
    ).

int16.min_int16 = -32768i16.

int16.(V_4 mod V_5) = V_3 :-
    V_3 = int16.(V_4 - V_6),
    V_6 = int16.(V_7 * V_5),
    V_7 = int16.(V_4 div V_5).

int16.nabs(V_3) = V_2 :-
    ( if
      V_4 = 0i16 : int16,
      int16.(V_3 > V_4)
    then
      V_2 = int16.(- V_3)
    else
      V_2 = V_3
    ).

int16.num_leading_zeros(V_3) = V_4 :-
    V_5 = uint16.cast_from_int16(V_3),
    V_4 = uint16.num_leading_zeros(V_5).

int16.num_ones(V_3) = V_4 :-
    V_5 = uint16.cast_from_int16(V_3),
    V_4 = uint16.num_ones(V_5).

int16.num_trailing_zeros(V_3) = V_4 :-
    V_5 = uint16.cast_from_int16(V_3),
    V_4 = uint16.num_trailing_zeros(V_5).

int16.num_zeros(V_3) = V_2 :-
    V_2 = int.(V_4 - V_5),
    V_4 = 16 : int,
    V_5 = int16.num_ones(V_3).

:- pragma inline((int16.odd)/1).
int16.odd(V_2) :-
    not (
      V_3 = int16.(V_2 /\ V_4),
      V_4 = 1i16 : int16,
      V_3 = 0i16 : int16
    ).

:- pragma inline((int16.(rem))/2).
int16.(V_4 rem V_5) = V_6 :-
    ( if
      V_5 = 0i16 : int16
    then
      V_7 = math.domain_error(V_8) : math.domain_error,
      V_8 = "int16.rem: division by zero" : string,
      exception.throw(V_7)
    else
      V_6 = int16.unchecked_rem(V_4, V_5)
    ).

int16.reverse_bits(V_3) = V_4 :-
    V_5 = uint16.cast_from_int16(V_3),
    V_6 = uint16.reverse_bits(V_5),
    V_4 = int16.cast_from_uint16(V_6).

