    Bits = ML_BITS_PER_UINT;
").

:- pragma foreign_proc("C", uint.cast_from_int(I :: (builtin.in)) = (U :: (builtin.out)), [will_not_call_mercury, thread_safe, not_tabled_for_io, promise_pure, will_not_modify_trail, doesnt_affect_liveness], "
