:- instance (enum.uenum(uint)).

:- pragma inline(func((uint.(/))/2)).
uint.(V_4 / V_5) = V_3 :-
    V_3 = uint.(V_4 // V_5).

:- pragma inline(func((uint.(//))/2)).
uint.(V_4 // V_5) = V_6 :-
    ( if
      V_5 = 0u : uint
    then
      V_7 = exception.domain_error(V_8) : exception.domain_error,
      V_8 = "uint.\'//\': division by zero" : string,
      exception.throw(V_7)
    else
      V_6 = uint.unchecked_quotient(V_4, V_5)
    ).

