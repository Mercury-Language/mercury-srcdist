:- module measurements.
:- interface.
:- type cs_cost_csq.
:- type goal_cost_csq.
:- type inherit_prof_info.
:- type is_active
	where	type_is_abstract_enum(1).
:- type own_prof_info.
:- type parallel_exec_metrics_incomplete.
:- type parallelism_amount.
:- type proc_cost_csq.
:- type recursion_depth.
:- type static_coverage_info.
:- type_representation(measurements.cs_cost_csq, is_word_aligned_ptr).
:- type_representation(measurements.inherit_prof_info, is_word_aligned_ptr).
:- type_representation(measurements.is_active, du_repn(enum("is_active", "is_not_active", [], []))).
:- type_representation(measurements.parallel_exec_metrics_incomplete, is_word_aligned_ptr).
:- type_representation(measurements.parallelism_amount, du_repn(notag("parallelism_amount", []))).
:- type_representation(measurements.proc_cost_csq, is_word_aligned_ptr).
:- type_representation(measurements.recursion_depth, du_repn(notag("recursion_depth", []))).
:- type_representation(measurements.static_coverage_info, is_eqv_to(maybe(array(int)))).
