:- module dump.
:- interface.
:- type dump_cliques
	where	type_is_abstract_enum(1).
:- type dump_options.
:- type dump_prop_measurements
	where	type_is_abstract_enum(1).
:- type dump_rev_links
	where	type_is_abstract_enum(1).
:- type dumpable_array
	where	type_is_abstract_enum(2).
:- type show_restricted_dump
	where	type_is_abstract_enum(1).
:- type show_stats
	where	type_is_abstract_enum(1).
:- type_representation(dump.dump_cliques, du_repn(enum("dump_cliques", "do_not_dump_cliques", [], []))).
