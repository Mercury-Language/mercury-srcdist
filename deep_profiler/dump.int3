:- type_representation(dump.dump_rev_links, du_repn(enum("dump_rev_links", "do_not_dump_rev_links", [], []))).
:- type_representation(dump.dumpable_array, du_repn(enum("csd", "css", ["pd", "ps"], []))).
:- type_representation(dump.show_restricted_dump, du_repn(enum("show_restricted_dump", "show_complete_dump", [], []))).
:- type_representation(dump.show_stats, du_repn(enum("show_stats", "do_not_show_stats", [], []))).
